#!/usr/bin/env ruby

require_relative '../lib/move.rb'
puts "Welcome to Tic Tac Toe!"
puts "Where would you like to go?"
user_input=gets.strip
board=[" ", " ", " ", " ", " ", " ", " ", " ", " "]
input_to_index(user_input="0")
input=user_input.to_i
spot=input-1
move(board, spot, value="X")
display_board(board)
# Code your CLI Here
